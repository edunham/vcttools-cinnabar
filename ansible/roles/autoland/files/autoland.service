[Unit]
Description=Autoland python daemon
After=syslog.target network.target auditd.service sshd.service

[Service]
User=autoland
Group=autoland
WorkingDirectory=/home/autoland/version-control-tools/autoland/autoland
ExecStart=/home/autoland/autoland_venv/bin/python autoland.py --log-path=/home/autoland/autoland.log

[Install]
WantedBy=multi-user.target

